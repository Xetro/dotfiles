*.font: xft:Hack Nerd Font Mono:size=9

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

URxvt*termName:                 rxvt-unicode
!URxvt.transparent:		true
!URxvt.shading:			30
!URxvt.tintColor:		white
urxvt*background:		rgba:0000/0000/0200/c800
